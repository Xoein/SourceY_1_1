<?xml version="1.0"?>
<project name="ant makefile invoker" default="compileCode" basedir=".">
  <target name="compileCode" description="Execute script and call make">
    <!-- exec dir="src/main/src" executable="/usr/bin/make" / -->
    <exec dir="src/" executable="make">
  	    <arg value="-f"/>
  	    <arg value="Makefile"/>
    </exec>
  </target>
</project>
